<?php

namespace Api\EmployerBundle\Entity;


class User
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $username;

    /**
     * @var string
     */
    private $password;

    /**
     * @var array
     */
    private $role;

    /**
     * @var Worker[]
     */
    private $works;
    
}
